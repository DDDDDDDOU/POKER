$message.success('取关 ' + this.name + ' 成功！');\n                }).catch(err=> {\n                    console.log(err);\n                });\n            },\n            home() {\n                if (this.$root.logged === true && this.id === Number(sessionStorage.getItem('id'))) {\n                    this.$router.push('/person');\n                }\n\n                else {\n                    this.$router.push({\n                        path: '/visit',\n                        query: {\n                            id: this.id\n                        }\n                    });\n                }\n            }\n        }\n    }\n</script>\n\n<style scoped>\n    .extern {\n        width: 40%;\n    }\n\n    .avatar {\n        margin-top: 8%;\n        width: 20%;\n        float: left;\n    }\n\n    .content {\n        width: 68%;\n        float: left;\n        margin-left: 5%;\n        margin-bottom: 5%;\n    }\n</style>\n"]}]}