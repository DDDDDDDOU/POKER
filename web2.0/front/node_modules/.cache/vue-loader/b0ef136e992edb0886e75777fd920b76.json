odel=\"blog.content.text\" :placeholder=\"holder\"/>\n            </div>\n        </div>\n        <div class=\"foot\">\n            <div class=\"check\">\n                <el-checkbox v-model=\"checked\">同时评论给 {{this.$props.user}}</el-checkbox>\n            </div>\n            <div class=\"btn\">\n                <el-button type=\"primary\" @click=\"submit\" size=\"mini\">发布</el-button>\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\n    export default {\n        name:\"share\",\n        props: {\n            id: Number,\n            user: String,\n            content: String,\n        },\n        data() {\n            return {\n                comment_to_flag: false,\n                holder: '//@',\n                active_tab: '1',\n                blog: {\n                    id: 0,\n                    user: {\n                        name: '交通大学',\n                        avatar: {\n                            // default avatar, must be iconBase64 mode\n                            iconBase64: 'https://cube.elemecdn.com/0/88/03b0d39583f48206768a7534e55bcpng.png'\n                        }\n                    },\n                    time: '',\n                    content: {\n                        text: '',\n                        images: []\n                    },\n                    comment: {\n                        count: 0,\n                    },\n                }\n            }\n        },\n        mounted() {\n            this.holder += this.$props.user + ':转发动态';\n        },\n        methods: {\n            submit() {\n                this.$message.success('转发成功！');\n\n                this.$emit('change',);\n                return true;\n\n            }\n        }\n    }\n</script>\n\n<style scoped>\n    .share {\n        width: 100%;\n        background-color: white;\n        flex-direction: column;\n    }\n\n    .quote {\n        width: 100%;\n        text-align: left;\n    }\n\n    .tabs {\n        margin-top: -30px;\n        height: 30px;\n    }\n\n    .main {\n        margin-top: 20px;\n    }\n\n    .quote {\n        background-color: #F2F2F5;\n    }\n\n    .text-overflow {\n        -o-text-overflow: ellipsis;\n        text-overflow: ellipsis;\n        height: 40%;\n        overflow: hidden;\n        white-space: nowrap;\n    }\n\n    .comment {\n        margin-top: 10px;\n    }\n\n    .foot {\n        margin-top: 10px;\n        margin-bottom: -10px;\n        width: 100%;\n        height: 40px;\n    }\n\n    .check {\n        float: left;\n    }\n\n    .btn {\n        float: right;\n    }\n</style>\n"]}]}