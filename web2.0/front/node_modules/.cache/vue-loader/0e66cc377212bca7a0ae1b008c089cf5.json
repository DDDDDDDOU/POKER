    console.log(err);\n            });\n        }\n    }\n</script>\n\n<style scoped>\n    .extern {\n        width: 100%;\n        background-color: #A7CFE8;\n    }\n\n    .person {\n        width: 90%;\n        display: flex;\n        flex-direction: column;\n        margin-left: 5%;\n    }\n\n    .head {\n        width: 100%;\n    }\n\n    .container {\n        width: 90%;\n        margin-left: 5%;\n        margin-top: 20px;\n    }\n\n    .card {\n        width: 80%;\n        margin-left: 10%;\n    }\n\n    .side {\n        float: left;\n        width: 20%;\n    }\n\n    .info {\n        margin-top: 10px;\n    }\n\n    .footstep {\n        margin-top: 10px;\n    }\n\n    .main {\n        float: left;\n        margin-left: 3%;\n        width: 77%;\n    }\n</style>\n"]}]}