   name: 'weins',\n                userMongo: {\n                    avatar: 'https://cube.elemecdn.com/3/7c/3ea6beec64369c2642b92c6726f1epng.png'\n                }\n            }\n        },\n        created() {\n            // if (this.$root.my_person_center === true) {\n            //     console.log('nmsl')\n            // }\n\n            this.id = this.$props.uid;\n            let url = 'http://localhost:8088/user/getPlainOne?id=' + this.id;\n\n            axios.get(url, {\n                headers: {\n                    token: sessionStorage.getItem(\"token\")\n                }\n            }).then((response) =>{\n                this.name = response.data.name;\n                this.userMongo = response.data.userMongo;\n            }).catch(err=> {\n                console.log(err);\n            });\n        },\n        methods: {\n            un_follow() {\n                let url = 'http://localhost:8088/user/follow?' +\n                    'sub=' + sessionStorage.getItem(\"id\") +\n                    '&obj=' + this.id +\n                    '&flag=-1';\n\n                axios.post(url, {}, {\n                    headers: {\n                        token: sessionStorage.getItem(\"token\")\n                    }\n                }).then((response) => {\n                   if (response.data === 'success')\n                       this.$message.success('取关 ' + this.name + ' 成功！');\n                }).catch(err=> {\n                    console.log(err);\n                });\n            },\n            home() {\n                if (this.$root.logged === true && this.id === Number(sessionStorage.getItem('id'))) {\n                    this.$router.push('/person');\n                }\n\n                else {\n                    this.$router.push({\n                        path: '/visit',\n                        query: {\n                            id: this.id\n                        }\n                    });\n                }\n            }\n        }\n    }\n</script>\n\n<style scoped>\n    .extern {\n        width: 40%;\n    }\n\n    .avatar {\n        margin-top: 8%;\n        width: 20%;\n        float: left;\n    }\n\n    .content {\n        width: 68%;\n        float: left;\n        margin-left: 5%;\n        margin-bottom: 5%;\n    }\n</style>\n"],"sourceRoot":"src/components"}]}