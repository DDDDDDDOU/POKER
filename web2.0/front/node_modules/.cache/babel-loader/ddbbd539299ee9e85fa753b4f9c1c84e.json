A,WAAA,EAAA,MAAA,CAAA;AACA,OAFA,CAAA,GAEA,KAFA;AAGA,MAAA,YAAA,CAAA,KAAA,OAAA,CAAA;AACA,WAAA,OAAA,GAAA,UAAA,CAAA,YAAA;AACA,QAAA,GAAA,CAAA,OAAA,CAAA;AACA,OAFA,EAEA,OAAA,IAAA,CAAA,MAAA,EAFA,CAAA;AAIA,aAAA,OAAA;AACA,KAnBA;AAoBA,IAAA,YApBA,wBAoBA,IApBA,EAoBA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,IAAA;AACA,aAAA,IAAA;AACA,KAvBA;AAwBA,IAAA,GAxBA,eAwBA,CAxBA,EAwBA;AACA,UAAA,CAAA,KAAA,CAAA,EAAA;AACA,aAAA,aAAA,GAAA,IAAA;AACA,eAAA,CAAA;AACA,OAHA,MAIA,IAAA,CAAA,KAAA,CAAA,EAAA;AACA,aAAA,OAAA,CAAA,IAAA,CAAA,SAAA;AACA,eAAA,CAAA;AACA,OAHA,MAIA,IAAA,CAAA,KAAA,CAAA,EAAA;AACA;AACA,aAAA,KAAA,CAAA,MAAA;AACA,aAAA,OAAA,CAAA,IAAA,CAAA,OAAA;AACA,QAAA,cAAA,CAAA,KAAA;AACA,aAAA,QAAA,CAAA,OAAA,CAAA,KAAA;AACA,eAAA,CAAA;AACA,OAPA,MAQA,OAAA,CAAA,CAAA;AACA;AA1CA,GAvBA;AAmEA,EAAA,OAnEA,qBAmEA;AAEA,SAAA,KAAA,GAAA,CAAA;AAAA,eAAA,YAAA;AAAA,iBAAA;AAAA,KAAA,EACA;AAAA,eAAA,qBAAA;AAAA,iBAAA;AAAA,KADA,EAEA;AAAA,eAAA,QAAA;AAAA,iBAAA;AAAA,KAFA,EAGA;AAAA,eAAA,YAAA;AAAA,iBAAA;AAAA,KAHA,EAIA;AAAA,eAAA,gBAAA;AAAA,iBAAA;AAAA,KAJA,EAKA;AAAA,eAAA,IAAA;AAAA,iBAAA;AAAA,KALA,EAMA;AAAA,eAAA,YAAA;AAAA,iBAAA;AAAA,KANA,EAOA;AAAA,eAAA,aAAA;AAAA,iBAAA;AAAA,KAPA,CAAA;AAQA,SAAA,OAAA;AACA;AA9EA,CAAA","sourcesContent":["<template>\n    <div style=\"z-index: 100\">\n        <div class=\"container\">\n            <div class=\"logo\">\n                <img class=\"logo img\" src='../assets/image/logo.png' alt=\"logo picture\"/>\n                <span class=\"tit\">WeIns</span>\n            </div>\n            <div class=\"searchbar\">\n                <el-autocomplete\n                        v-model=\"state\"\n                        :fetch-suggestions=\"querySearchAsync\"\n                        placeholder=\"请输入内容\"\n                        @select=\"handleSelect\"\n                        size=\"min\"\n                        style=\"width: 40%\"\n                ></el-autocomplete>\n                <el-button type=\"primary\" icon=\"el-icon-search\" v-on:click=\"log(0)\">搜索</el-button>\n            </div>\n            <div class=\"nav\">\n\n\n                <div class=\"el-dropdown-link\">\n                    <router-link to=\"/home\">\n                        <p class=\"el-dropdown-link can-point\"><i class=\"el-icon-s-home el-icon--left\"></i>首页</p>\n                    </router-link>\n                </div>\n                <span class=\"dd\">|</span>\n<!--                <router-link to=\"/manager\" class=\"el-dropdown-link\">-->\n                    <p class=\"el-dropdown-link can-point\" ><i class=\"el-icon-search el-icon--left\"></i>发现</p>\n<!--                </router-link>-->\n                <span class=\"dd\" v-if=\"this.$root.logged === true\">|</span>\n\n\n                <div class=\"el-dropdown-link\" v-if=\"this.$root.logged === true\">\n                    <el-dropdown  trigger=\"click\">\n                    <span class=\"el-dropdown-link\">\n                        我的<i class=\"el-icon-arrow-down el-icon--right\"></i>\n                    </span>\n                        <el-dropdown-menu slot=\"dropdown\" style=\"width: 100px\">\n                            <router-link to=\"/person\">\n                                <span class=\"el-dropdown-link can-point\" id=\"person\"><el-dropdown-item>个人信息</el-dropdown-item></span>\n                            </router-link>\n                            <router-link to=\"/manager\" v-if=\"this.$root.is_superuser\">\n                                <span class=\"el-dropdown-link can-point\" ><el-dropdown-item>用户管理</el-dropdown-item></span>\n                            </router-link>\n                            <router-link to=\"/person\">\n                                <span class=\"el-dropdown-link can-point\" id=\"footprint\"><el-dropdown-item>我的足迹</el-dropdown-item></span>\n                            </router-link>\n                        </el-dropdown-menu>\n                    </el-dropdown>\n                </div>\n\n\n                <span class=\"dd\">|</span>\n                <div class=\"el-dropdown-link\">\n                    <el-dropdown trigger=\"click\">\n                        <p class=\"el-dropdown-link\">\n                            用户服务<i class=\"el-icon-arrow-down el-icon--right\"></i>\n                        </p>\n                        <el-dropdown-menu slot=\"dropdown\" style=\"width: 80px;font-size: 10px\">\n                            <p v-if=\"!this.$root.logged\" style=\"width: 80px\" v-on:click=\"log(0)\"><el-dropdown-item >登陆</el-dropdown-item></p>\n                            <p v-if=\"!this.$root.logged\" style=\"width: 80px\" v-on:click=\"log(1)\"><el-dropdown-item>注册</el-dropdown-item></p>\n                            <p v-if=\"this.$root.logged\" style=\"width: 80px\" v-on:click=\"log(2)\"><el-dropdown-item>注销</el-dropdown-item></p>\n                        </el-dropdown-menu>\n                    </el-dropdown>\n                </div>\n\n\n            </div>\n        </div>\n\n        <el-dialog :visible.sync=\"dialogVisible\" width=\"40%\" :show-close=\"false\" v-if=\"this.$root.logged === false\">\n            <Login></Login>\n        </el-dialog>\n\n    </div>\n</template>\n\n<script>\n    import Login from \"./signinForm\";\n    export default {\n        components: { Login },\n        data() {\n            return {\n                items: [],\n                state: '',\n                timeout: null,\n                activeIndex: '1',\n                activeIndex2: '1',\n                showitems:[],\n                dialogVisible: false,\n                any: [{\n                    value: 'huangjingao',\n                    label: '黄金糕',\n                }, {\n                    value: 'shizitou',\n                    label: '狮子头',\n                }, {\n                    value: 'luosifen',\n                    label: '螺蛳粉',\n                }]\n            };\n        },\n        methods: {\n            loadAll() {\n                const url=\"https://localhost:8088/ddd\"\n                return this.axios.post(url).then(res=>{\n                    return res === 'success';\n                })\n            },\n            querySearchAsync(queryString,cbe) {\n\n                let items = this.items;\n                let results = queryString ? items.filter((state) => {\n                    return (state.value.toLowerCase().indexOf(queryString.toLowerCase()) === 0);\n                }) : items;\n                    clearTimeout(this.timeout);\n                    this.timeout = setTimeout(() => {\n                        cbe(results);\n                    }, 3000 * Math.random());\n\n               return results;\n            },\n            handleSelect(item) {\n                console.log(item);\n                return item;\n            },\n            log(i) {\n                if(i === 0){\n                    this.dialogVisible = true;\n                    return 0;\n                }\n                else if (i === 1) {\n                    this.$router.push(\"/signup\");\n                    return 1;\n                }\n                else if(i === 2){\n                    //注销\n                    this.$root.logout();\n                    this.$router.push(\"/home\");\n                    sessionStorage.clear();\n                    this.$message.success(\"已注销\");\n                    return 2;\n                }\n                else return -1;\n            }\n        },\n        mounted() {\n\n            this.items=[{\"value\": \"三全鲜食（北新泾店）\", \"address\": \"长宁区新渔路144号\"},\n            {\"value\": \"Hot honey 首尔炸鸡（仙霞路）\", \"address\": \"上海市长宁区淞虹路661号\"},\n            {\"value\": \"新旺角茶餐厅\", \"address\": \"上海市普陀区真北路988号创邑金沙谷6号楼113\"},\n            {\"value\": \"泷千家(天山西路店)\", \"address\": \"天山西路438号\"},\n            {\"value\": \"胖仙女纸杯蛋糕（上海凌空店）\", \"address\": \"上海市长宁区金钟路968号1幢18号楼一层商铺18-101\"},\n            {\"value\": \"贡茶\", \"address\": \"上海市长宁区金钟路633号\"},\n            {\"value\": \"豪大大香鸡排超级奶爸\", \"address\": \"上海市嘉定区曹安公路曹安路1685号\"},\n            {\"value\": \"茶芝兰（奶茶，手抓饼）\", \"address\": \"上海市普陀区同普路1435号\"}];\n            this.loadAll();\n        },\n    }\n</script>\n\n<style scoped>\n    .tit {\n        height: 50px;\n        font-size: 20px;\n        color: coral;\n        line-height: 50px;\n        font-family: \"Al Bayan\",serif;\n        width: 50%;\n    }\n    .container {\n        width: 100%;\n        float: left;\n        background-color: antiquewhite;\n        left: 0;\n        right: 0;\n        position: fixed;\n        z-index: 999;\n    }\n    .logo {\n        width: 10%;\n        height: 10%;\n        float: left;\n        margin-left: 3%;\n        padding: 0;\n        text-align: center;\n    }\n    .logo .img {\n        width: 50px;\n    }\n    body {\n        margin: 0;\n        padding: 0;\n    }\n    .searchbar {\n        width: 35%;\n        float: left;\n        height: 50px;\n        line-height: 50px;\n        text-align: center;\n    }\n    el-autocomplete {\n        width: 10%\n    }\n    .nav {\n        width: 45%;\n        float: left;\n        text-align: center;\n        height: 50px;\n    }\n    el-menu {\n        height: 50px;\n        background-color: antiquewhite;\n    }\n    .el-dropdown-link {\n        font-size: 15px;\n        line-height: 50px;\n        height: 50px;\n        width:100px;\n        float: left;\n    }\n    .el-dropdown-link:hover {\n        font-size: 17px;\n    }\n    .can-point {\n        cursor: pointer;\n    }\n    .dd {\n        line-height: 50px;\n        font-size: 15px;\n        font-family: Arial, sans-serif;\n        float: left;\n    }\n</style>\n"],"sourceRoot":"src/components"}]}