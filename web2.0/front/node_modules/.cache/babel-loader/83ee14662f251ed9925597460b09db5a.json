AoFA,IAAA,MApFA,oBAoFA;AACA,WAAA,UAAA,GAAA,KAAA;AACA,aAAA,IAAA;AACA,KAvFA;AAwFA,IAAA,OAxFA,qBAwFA;AAAA;;AACA,UAAA,KAAA,KAAA,CAAA,MAAA,KAAA,KAAA,EAAA;AACA,aAAA,QAAA,CAAA,IAAA,CAAA,WAAA;AACA,eAAA,KAAA;AACA;;AAEA,UAAA,GAAA,GAAA,uCACA,OADA,GACA,cAAA,CAAA,OAAA,CAAA,IAAA,CADA,GAEA,OAFA,GAEA,KAAA,IAAA,CAAA,EAFA,GAGA,QAHA;;AAKA,UAAA,KAAA,YAAA,EAAA;AACA,QAAA,KAAA,CAAA,GAAA,CAAA,GAAA,GAAA,OAAA,EAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,cAAA,QAAA,CAAA,IAAA,KAAA,IAAA,EAAA;AACA,YAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,OAAA;;AACA,YAAA,MAAA,CAAA,IAAA,CAAA,WAAA;AACA,YAAA,MAAA,CAAA,YAAA,GAAA,KAAA;AACA,mBAAA,KAAA;AACA;AACA,SAPA,EAOA,KAPA,CAOA,UAAA,GAAA,EAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,SATA;AAUA,OAXA,MAaA;AACA,QAAA,KAAA,CAAA,GAAA,CAAA,GAAA,GAAA,MAAA,EAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,cAAA,QAAA,CAAA,IAAA,KAAA,IAAA,EAAA;AACA,YAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,OAAA;;AACA,YAAA,MAAA,CAAA,YAAA,GAAA,IAAA;AACA,YAAA,MAAA,CAAA,IAAA,CAAA,WAAA;AACA,mBAAA,IAAA;AACA;AACA,SAPA,EAOA,KAPA,CAOA,UAAA,GAAA,EAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,SATA;AAUA;AACA,KA5HA;AA6HA,IAAA,IA7HA,kBA6HA;AAAA;;AACA,UAAA,KAAA,KAAA,CAAA,MAAA,KAAA,KAAA,EAAA;AACA,aAAA,QAAA,CAAA,IAAA,CAAA,WAAA;AACA,eAAA,KAAA;AACA;;AAEA,UAAA,KAAA,SAAA,EAAA;AACA,YAAA,GAAA,GAAA,2CACA,MADA,GACA,cAAA,CAAA,OAAA,CAAA,IAAA,CADA,GAEA,OAFA,GAEA,KAAA,IAAA,CAAA,EAFA;AAIA,QAAA,KAAA,CAAA,GAAA,CAAA,GAAA,EAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,cAAA,QAAA,CAAA,IAAA,KAAA,IAAA,EAAA;AACA,YAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,MAAA;;AACA,YAAA,MAAA,CAAA,SAAA,GAAA,KAAA;AACA,YAAA,MAAA,CAAA,IAAA,CAAA,IAAA;AACA,mBAAA,KAAA;AACA,WALA,MAMA;AACA,YAAA,OAAA,CAAA,GAAA,CAAA,aAAA;AACA;AACA,SAVA,EAUA,KAVA,CAUA,UAAA,GAAA,EAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,SAZA;AAcA,OAnBA,MAqBA;AACA,YAAA,IAAA,GAAA,qCACA,MADA,GACA,cAAA,CAAA,OAAA,CAAA,IAAA,CADA,GAEA,OAFA,GAEA,KAAA,IAAA,CAAA,EAFA;;AAIA,QAAA,KAAA,CAAA,GAAA,CAAA,IAAA,EAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,cAAA,QAAA,CAAA,IAAA,KAAA,IAAA,EAAA;AACA,YAAA,MAAA,CAAA,SAAA,GAAA,IAAA;AACA,YAAA,MAAA,CAAA,IAAA,CAAA,IAAA;;AACA,YAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,OAAA;;AACA,mBAAA,IAAA;AACA,WALA,MAOA;AACA,YAAA,OAAA,CAAA,GAAA,CAAA,aAAA;AACA,mBAAA,KAAA;AACA;AACA,SAZA,EAYA,KAZA,CAYA,UAAA,GAAA,EAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,SAdA;AAeA;AACA,KA7KA;AA8KA,IAAA,KA9KA,mBA8KA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AACA,QAAA,IAAA,EAAA,QADA;AAEA,QAAA,KAAA,EAAA;AACA,UAAA,EAAA,EAAA,KAAA,IAAA,CAAA;AADA;AAFA,OAAA;AAMA,KArLA;AAsLA,IAAA,MAtLA,kBAsLA,KAtLA,EAsLA;AACA,WAAA,aAAA,GAAA,IAAA;AACA,WAAA,OAAA,GAAA,IAAA,CAAA,KAAA,CAAA,KAAA,EAAA,MAAA;AACA,aAAA,IAAA;AACA,KA1LA;AA2LA,IAAA,OA3LA,qBA2LA;AACA,UAAA,KAAA,KAAA,CAAA,MAAA,KAAA,KAAA,EAAA;AACA,aAAA,QAAA,CAAA,IAAA,CAAA,WAAA;AACA,eAAA,KAAA;AACA;;AACA,WAAA,YAAA,GAAA,IAAA;AACA,aAAA,IAAA;AACA;AAlMA;AA3BA,CAAA","sourcesContent":["<template>\n    <div style=\"width: 100%\" class=\"blog_div1\">\n        <el-card class=\"extern\" shadow=\"hover\" >\n            <div class=\"blog\" style=\"width: 100%\">\n                <div class=\"avatar\">\n                    <el-avatar class=\"blog_avatar\" :src=\"data.userAvatar\"></el-avatar>\n                </div>\n\n                <div class=\"container\">\n                    <div class=\"header\" style=\"z-index: 998;\">\n                        <el-row style=\"height: 20px\">\n                            <el-col :span=\"20\">\n                                <div class=\"name\">\n                                    <el-button type=\"text\" style=\"float: left\" @click=\"visit\">{{generator(data)}}</el-button>\n                                </div>\n                                <div class=\"timestamp\">{{blog.post_day}}</div>\n                                <div class=\"timestamp\">当前可见：{{parseType2Str()}}</div>\n                            </el-col>\n\n                            <el-col :span=\"3\" style=\"float: right\">\n                                <div>\n                                    <el-dropdown trigger=\"click\" style=\"outline: none\">\n                                    <span\n                                            class=\"el-dropdown-link btn send time-send small-hand\"\n                                            id=\"custom-send-btn\"\n                                    >选项<i class=\"el-icon-arrow-down el-icon--right\"></i>\n                                    </span>\n\n                                        <el-dropdown-menu slot=\"dropdown\" style=\"width: 12%\">\n\n                                            <p v-on:click=\"option(0)\" class=\"menuitem\" v-if=\"blog.type !== 0\">\n                                                <el-dropdown-item >设置权为私密</el-dropdown-item>\n                                            </p>\n\n                                            <p v-on:click=\"option(3)\" class=\"menuitem\" v-if=\"blog.type !== 3\">\n                                                <el-dropdown-item>设置为所有人可见</el-dropdown-item>\n                                            </p>\n\n                                            <p v-on:click=\"option(1)\" class=\"menuitem\" v-if=\"blog.type !== 1\">\n                                                <el-dropdown-item>设置为好友可见</el-dropdown-item>\n                                            </p>\n\n                                            <p v-on:click=\"option(2)\" class=\"menuitem\" v-if=\"this.$root.is_superuser === true\">\n                                                <el-dropdown-item>删除</el-dropdown-item>\n                                            </p>\n\n                                        </el-dropdown-menu>\n\n                                    </el-dropdown>\n                                </div>\n\n                            </el-col>\n                        </el-row>\n                    </div>\n\n                    <div class=\"content\" style=\"z-index: 998;\">\n                        <div class=\"text\">\n                            {{blogMongo.content}}\n                        </div>\n                        <div class=\"images\" v-if=\"blogMongo.images !== null\">\n                            <ul>\n                                <li style=\"\" v-for=\"image in data.blogMongo.images\" :key=\"image\">\n                                    <img @click=\"maxPic(image)\" :src=\"parseBase64(image)\" class=\"img\" style=\"z-index: 998\"/>\n                                </li>\n                            </ul>\n                        </div>\n                    </div>\n                </div>\n\n                <div class=\"footer\" style=\"text-align: center\">\n                    <el-row>\n                        <el-col :span=\"6\">\n                            <div v-if=\"collect_flag === true\">\n                                <el-button type=\"text\" icon=\"el-icon-folder-remove\" @click=\"collect\">{{blog.coll_number}}\n                                </el-button>\n                            </div>\n                            <div v-else>\n                                <el-button type=\"text\" icon=\"el-icon-folder-add\" @click=\"collect\">{{blog.coll_number}}\n                                </el-button>\n                            </div>\n                        </el-col>\n                        <el-col :span=\"6\">\n                            <el-button type=\"text\" icon=\"el-icon-top-right\" @click=\"share\">{{blog.reblog}}</el-button>\n                        </el-col>\n                        <el-col :span=\"6\">\n                            <el-button type=\"text\" icon=\"el-icon-chat-dot-square\" @click=\"comment\">\n                                {{blog.com_number}}\n                            </el-button>\n                        </el-col>\n                        <el-col :span=\"6\">\n                            <div v-if=\"like_flag === true\">\n                                <el-button type=\"text\" icon=\"el-icon-star-on\" @click=\"like\">{{blog.like}}</el-button>\n                            </div>\n                            <div v-else>\n                                <el-button type=\"text\" icon=\"el-icon-star-off\" @click=\"like\">{{blog.like}}</el-button>\n                            </div>\n                        </el-col>\n                    </el-row>\n\n                    <el-dialog class=\"blog_dialog1\" :append-to-body=\"true\" :visible.sync=\"share_flag\" width=\"40%\" :show-close=\"false\"\n                               title=\"转发动态到\">\n                        <share :id=\"blog.id\" :user=\"blog.username\" :content=\"blogMongo.content\"\n                               @change=\"change\"></share>\n                    </el-dialog>\n                    <el-dialog class=\"blog_dialog2\" :visible.sync=\"dialogVisible\" width=\"40%\" :show-close=\"false\" :append-to-body=\"true\"\n                               style=\"z-index: 999\">\n                        <el-image :src=\"this.showpic\" class=\"big-img\"></el-image>\n                    </el-dialog>\n\n                    <el-dialog class=\"blog_dialog3\" :append-to-body=\"true\" :visible.sync=\"comment_flag\" width=\"60%\" :show-close=\"false\">\n                        <release_comment :bid=\"this.blog.id\" :to_uid=\"this.blog.uid\" :to_username=\"this.blog.username\"></release_comment>\n                        <comment></comment>\n                    </el-dialog>\n                </div>\n            </div>\n        </el-card>\n    </div>\n\n</template>\n\n<script>\n    import axios from 'axios';\n\n    import share from '../components/share';\n    import comment from \"./comment\";\n    import release_comment from \"./release_comment\";\n\n    export default {\n        components: {\n            share, comment, release_comment\n        },\n        props: {\n            data: Object\n        },\n        data() {\n            return {\n                blog: {},\n                blogMongo: {},\n                comments: [],\n                reblog: {},\n                reblogMongo: {},\n                dialogVisible: false,\n                showpic: \"\",\n\n                like_num: 0,\n                like_flag: false,\n                collect_num: 0,\n                collect_flag: false,\n                share_num: 0,\n                share_flag: false,\n                comment_num: 0,\n                comment_flag: false\n            }\n        },\n        methods: {\n            // 拉取数据\n            generator(val) {\n                this.blog = val.blog;\n                this.blogMongo = val.blogMongo;\n                this.comments = val.comments;\n                this.comment_num = this.comments.length;\n\n                console.log(val);\n                console.log(\"nmsl\")\n                console.log(this.blog);\n\n                if (this.$root.logged === true) {\n                    let id = Number(sessionStorage.getItem(\"id\"));\n                    console.log(id);\n\n                    // collect\n                    let colllist = this.blogMongo.who_collect;\n                    for (let i = 0; i < colllist.length; i++) {\n                        if (colllist[i] === id) {\n                            this.collect_flag = true;\n                            break;\n                        }\n                    }\n\n                    let likelist = this.blogMongo.who_like;\n                    for (let i = 0; i < likelist.length; i++) {\n                        if (likelist[i] === id) {\n                            this.like_flag = true;\n                            break;\n                        }\n                    }\n                }\n                return val.blog.username;\n            },\n            parseBase64(image) {\n                return JSON.parse(image).base64;\n            },\n            parseType2Str() {\n                switch (this.blog.type) {\n                    case 0:\n                        return '仅自己可见';\n                    case 1:\n                        return '粉丝可见';\n                    case 3:\n                        return '公开可见';\n                }\n            },\n            option(op) {\n                switch (op) {\n                    case 0: case 1: case 3:\n                        this.blog.type = op;\n                        break;\n                    case 2:\n                        this._delete();\n                        break;\n                    default:\n                        break;\n                }\n            },\n            _delete() {\n                let url = 'http://localhost:8088/blog/removeBlog?'\n                + 'uid=' + sessionStorage.getItem(\"id\")\n                + '&bid=' + this.blog.id\n                + '&type=' + this.blog.type;\n\n                axios.get(url).then((response) =>{\n                    if (response.data === true) {\n                        this.$message.success('删除成功！');\n                        this.$emit('delete')                    }\n\n                    else\n                        this.$message.error('没有权限删除！');\n                });\n            },\n            share() {\n                if (this.$root.logged === false) {\n                    this.$message.info(\"请登录后再进行操作\");\n                    return false;\n                }\n\n                this.share_flag = true;\n                return true;\n            },\n            change() {\n                this.share_flag = false;\n                return true;\n            },\n            collect() {\n                if (this.$root.logged === false) {\n                    this.$message.info(\"请登录后再进行操作\");\n                    return false;\n                }\n\n                let url = 'http://localhost:8088/blog/collect' +\n                    '?uid=' + sessionStorage.getItem('id') +\n                    '&bid=' + this.blog.id +\n                    '&flag=';\n\n                if (this.collect_flag) {\n                    axios.get(url + 'false').then((response) =>{\n                        if (response.data === true) {\n                            this.$message.error('取消收藏！');\n                            this.blog.coll_number--;\n                            this.collect_flag = false;\n                            return false;\n                        }\n                    }).catch(err => {\n                        console.log(err);\n                    });\n                }\n\n                else {\n                    axios.get(url + 'true').then((response) =>{\n                        if (response.data === true) {\n                            this.$message.success('收藏成功！');\n                            this.collect_flag = true;\n                            this.blog.coll_number++;\n                            return true;\n                        }\n                    }).catch(err => {\n                        console.log(err);\n                    });\n                }\n            },\n            like() {\n                if (this.$root.logged === false) {\n                    this.$message.info(\"请登录后再进行操作\");\n                    return false;\n                }\n\n                if (this.like_flag) {\n                    let url = 'http://localhost:8088/blog/removeLike?'\n                        + 'uid=' + sessionStorage.getItem(\"id\")\n                        + '&bid=' + this.blog.id;\n\n                    axios.get(url).then((response) =>{\n                        if (response.data === true) {\n                            this.$message.error('取消赞！');\n                            this.like_flag = false;\n                            this.blog.like--;\n                            return false;\n                        }\n                        else {\n                            console.log('have liked.');\n                        }\n                    }).catch(err => {\n                        console.log(err);\n                    });\n\n                }\n\n                else {\n                    let url = 'http://localhost:8088/blog/like?'\n                        + 'uid=' + sessionStorage.getItem(\"id\")\n                        + '&bid=' + this.blog.id;\n\n                    axios.get(url).then((response) =>{\n                        if (response.data === true) {\n                            this.like_flag = true;\n                            this.blog.like++;\n                            this.$message.success('点赞成功！');\n                            return true;\n                        }\n\n                        else {\n                            console.log('have liked.');\n                            return false;\n                        }\n                    }).catch(err => {\n                        console.log(err);\n                    });\n                }\n            },\n            visit() {\n                this.$router.push({\n                    path: '/visit',\n                    query: {\n                        id: this.blog.uid\n                    }\n                });\n            },\n            maxPic(image) {\n                this.dialogVisible = true;\n                this.showpic = JSON.parse(image).base64;\n                return true;\n            },\n            comment() {\n                if (this.$root.logged === false) {\n                    this.$message.info(\"请登录后再进行操作\");\n                    return false;\n                }\n                this.comment_flag = true;\n                return true;\n            },\n\n        },\n    }\n</script>\n\n<style scoped>\n    .extern {\n        width: 100%;\n    }\n\n    .blog {\n        width: 100%;\n        margin-top: -10px;\n    }\n\n    .avatar {\n        float: left;\n        width: 7%;\n    }\n\n    .container {\n        margin-left: 2%;\n        float: left;\n        width: 85%;\n        display: flex;\n        flex-direction: column;\n    }\n\n    .header {\n        width: 100%;\n    }\n\n    .name {\n        float: left;\n        width: 100%;\n        text-align: left;\n    }\n\n    .timestamp {\n        text-align: left;\n        color: darkgray;\n        font-size: 10px;\n    }\n\n    .content {\n        width: 100%;\n        margin-top: 5%;\n        background: rgba(255, 255, 255, 0.4);\n    }\n\n    .menuitem {\n    }\n\n    .text {\n        text-align: left;\n    }\n\n    .img {\n        width: 150px;\n        height: 150px;\n        float: left;\n        margin-left: 1%;\n        margin-top: 1%;\n        cursor: pointer;\n        object-fit: cover;\n    }\n\n    .big-img {\n        width: 100%;\n    }\n\n    .footer {\n        border-top: 2px #909399;\n        margin-top: 20px;\n    }\n</style>\n"],"sourceRoot":"src/components"}]}