A,KAAA,EAAA,EACA,GAAA,IAAA,GAAA;AAEA,MAAA,GAAA,IAAA,IAAA,CAAA,QAAA,KAAA,GAAA;AAEA,UAAA,IAAA,CAAA,UAAA,KAAA,EAAA,EACA,GAAA,IAAA,GAAA;AAEA,MAAA,GAAA,IAAA,IAAA,CAAA,UAAA,EAAA;AAEA,aAAA,GAAA;AACA,KAzBA;AA0BA,IAAA,MA1BA,oBA0BA;AAAA;;AACA,UAAA,GAAA,GAAA,sCAAA;AAEA,UAAA,IAAA,GAAA;AACA,QAAA,GAAA,EAAA,cAAA,CAAA,OAAA,CAAA,IAAA,CADA;AAEA,QAAA,GAAA,EAAA,KAAA,MAAA,CAAA,EAFA;AAGA,QAAA,IAAA,EAAA,KAAA,UAHA;AAIA,QAAA,OAAA,EAAA,KAAA,IAJA;AAKA,QAAA,QAAA,EAAA,KAAA,SAAA,EALA;AAMA,QAAA,QAAA,EAAA,KAAA,MAAA,CAAA;AANA,OAAA;AASA,MAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,IAAA;AAEA,MAAA,KAAA,CAAA,IAAA,CAAA,GAAA,EAAA;AACA,QAAA,GAAA,EAAA,cAAA,CAAA,OAAA,CAAA,IAAA,CADA;AAEA,QAAA,GAAA,EAAA,KAAA,MAAA,CAAA,EAFA;AAGA,QAAA,IAAA,EAAA,KAAA,UAHA;AAIA,QAAA,OAAA,EAAA,KAAA,IAJA;AAKA,QAAA,QAAA,EAAA,KAAA,SAAA,EALA;AAMA,QAAA,QAAA,EAAA,KAAA,MAAA,CAAA;AANA,OAAA,EAOA,IAPA,CAOA,UAAA,QAAA,EAAA;AACA,YAAA,QAAA,CAAA,IAAA,KAAA,IAAA,EACA,KAAA,CAAA,QAAA,CAAA,OAAA,CAAA,OAAA,EADA,KAGA,KAAA,CAAA,QAAA,CAAA,KAAA,CAAA,MAAA;AACA,OAZA,EAYA,KAZA,CAYA,UAAA,GAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,OAdA;AAgBA,WAAA,KAAA,CAAA,QAAA;AACA,aAAA,IAAA,CAhCA,CAiCA;AACA;AACA;AACA;AACA;AACA;AACA;AAjEA;AAlBA,CAAA","sourcesContent":["<template>\n    <div class=\"share\">\n        <div class=\"destination\">\n            <div class=\"tabs\">\n                <el-tabs v-model=\"active_tab\">\n                    <el-tab-pane label=\"我的动态\" name=\"3\"></el-tab-pane>\n                    <el-tab-pane label=\"好友圈\" name=\"1\"></el-tab-pane>\n                </el-tabs>\n            </div>\n        </div>\n        <div class=\"main\">\n            <div class=\"quote text-overflow\">\n                @{{this.$props.user}}: {{this.$props.content}}\n            </div>\n            <div class=\"comment\">\n                <el-input type=\"textarea\" v-model=\"text\" :placeholder=\"holder\"/>\n            </div>\n        </div>\n        <div class=\"foot\">\n            <div class=\"check\">\n                <el-checkbox v-model=\"comment_to_flag\">同时评论给 {{this.$props.user}}</el-checkbox>\n            </div>\n            <div class=\"btn\">\n                <el-button type=\"primary\" @click=\"submit\" size=\"mini\">发布</el-button>\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\n    import axios from 'axios';\n\n    export default {\n        name:\"share\",\n        props: {\n            id: Number,\n            user: String,\n            content: String,\n        },\n        data() {\n            return {\n                comment_to_flag: false,\n                holder: '//@',\n                active_tab: '3',\n                text: ''\n            }\n        },\n        created() {\n            this.holder += this.$props.user + ':转发动态';\n        },\n        methods: {\n            curr_time() {\n                let date = new Date();\n                let res = date.getFullYear() + '-';\n                if (date.getMonth() < 9)\n                    res += '0';\n                let month=date.getMonth()+1;\n                res += (month+ '-');\n\n                if (date.getDate() < 10)\n                    res += '0';\n\n                res += date.getDate() + ' ';\n\n                if (date.getHours() < 10)\n                    res += '0';\n\n                res += date.getHours() + ':';\n\n                if (date.getMinutes() < 10)\n                    res += '0';\n\n                res += date.getMinutes();\n\n                return res;\n            },\n            submit() {\n                let url = 'http://localhost:8088/blog/setReblog';\n\n                let form = {\n                    uid: sessionStorage.getItem(\"id\"),\n                    bid: this.$props.id,\n                    type: this.active_tab,\n                    content: this.text,\n                    post_day: this.curr_time(),\n                    username: this.$props.user\n                }\n\n                console.log(url);\n                console.log(form);\n\n                axios.post(url, {\n                    uid: sessionStorage.getItem(\"id\"),\n                    bid: this.$props.id,\n                    type: this.active_tab,\n                    content: this.text,\n                    post_day: this.curr_time(),\n                    username: this.$props.user\n                }).then((response) => {\n                    if (response.data === true)\n                        this.$message.success('转发成功！');\n                    else\n                        this.$message.error('转发失败');\n                }).catch(err => {\n                    console.log(err);\n                });\n\n                this.$emit('change',);\n                return true;\n                // Integer uid;\n                // Integer bid;\n                // Integer type;\n                // String content;\n                // String post_day;\n                // String username;\n            }\n        }\n    }\n</script>\n\n<style scoped>\n    .share {\n        width: 100%;\n        background-color: white;\n        flex-direction: column;\n    }\n\n    .quote {\n        width: 100%;\n        text-align: left;\n    }\n\n    .tabs {\n        margin-top: -30px;\n        height: 30px;\n    }\n\n    .main {\n        margin-top: 20px;\n    }\n\n    .quote {\n        background-color: #F2F2F5;\n    }\n\n    .text-overflow {\n        -o-text-overflow: ellipsis;\n        text-overflow: ellipsis;\n        height: 40%;\n        overflow: hidden;\n        white-space: nowrap;\n    }\n\n    .comment {\n        margin-top: 10px;\n    }\n\n    .foot {\n        margin-top: 10px;\n        margin-bottom: -10px;\n        width: 100%;\n        height: 40px;\n    }\n\n    .check {\n        float: left;\n    }\n\n    .btn {\n        float: right;\n    }\n</style>\n"],"sourceRoot":"src/components"}]}